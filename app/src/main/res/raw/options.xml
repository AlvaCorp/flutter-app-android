<?xml version="1.0" encoding="UTF-8"?>
<Guide>

	<GlobalOptions>
		<Node>
			<Id>output_options</Id>
			<Text>'s': servo</Text>
			<Text>'r': red LED</Text>
			<Text>'g': green LED</Text>
			<Text>'b': blue LED</Text>
			<Text>'v': volume of the buzzer</Text>
			<Text>'f': frequency of the buzzer</Text>
		</Node>
		<Node>
			<Id>which_one</Id>
			<Text>1</Text>
			<Text>2</Text>
			<Text>3</Text>
		</Node>
		<Node>
			<Id>range_v</Id>
			<Text>range: 0-100</Text>
		</Node>
		<Node>
			<Id>range_f</Id>
			<Text>range: 0-20000</Text>
		</Node>
	</GlobalOptions>

	<GuideOptions>

		<!-- Root -->

		<Node>
			<Name>main_prompt</Name>
			<Title>What would you like to do?</Title>
			<Option>'r': read sensors</Option>
			<Option>'R': stream sensor readings for 10 seconds</Option>
			<Option>'s': set an output</Option>
			<Option>'p': set a proportional relationship</Option>
			<Option>'x': remove a relationship</Option>
			<Option>'X': remove relationships for all outputs</Option>
		</Node>

		<!-- First Generation -->
	
		<Node>
			<Name>read_sensors</Name>
			<Title>Let's read the sensors!</Title>
		</Node>

		<Node>
			<Name>stream_sensors</Name>
			<Title>Streaming sensors for 10 seconds!</Title>
		</Node>

		<Node>
			<Name>set_output</Name>
			<Title>What type of output?</Title>
			<Option>output_options</Option>
		</Node>

		<Node>
			<Name>set_proportion</Name>
			<Title>What type of output?</Title>
			<Option>output_options</Option>
		</Node>

		<Node>
			<Name>remove_relationship</Name>
			<Title>Choose one</Title>
			<Option>output_options</Option>
		</Node>

		<Node>
			<Name>remove_all_relationships</Name>
			<Title>Let's remove all relationships!</Title>
		</Node>

		<!-- Second Generation -->

			<!-- set_output -->

			<Node>
				<Name>output_servo</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>output_red</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>output_green</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>
			<Node>

				<Name>output_blue</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>output_volume</Name>
				<Title>What would you like to set the volume value to?</Title>
				<Option>range_v</Option>
			</Node>

			<Node>
				<Name>output_frequency</Name>
				<Title>What would you like to set the frequency value to?</Title>
				<Option>range_f</Option>
			</Node>

			<!-- set_proportion -->

			<Node>
				<Name>proportion_servo</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>proportion_red</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>proportion_green</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>proportion_blue</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>proportion_volume</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>proportion_frequency</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<!-- remove_relationship -->

			<Node>
				<Name>remove_servo</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>remove_red</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>remove_green</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>remove_blue</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>remove_volume</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>

			<Node>
				<Name>remove_frequency</Name>
				<Title>Which one?</Title>
				<Option>which_one</Option>
			</Node>
		
	</GuideOptions>
	
	<Linking>

		<!-- First Linking -->
	
		<Node>
			<Parent>main_prompt</Parent>
			<Child>read_sensors</Child>
			<Child>stream_sensors</Child>
			<Child>set_output</Child>
			<Child>set_proportion</Child>
			<Child>remove_relationship</Child>
			<Child>remove_all_relationships</Child>
		</Node>

		<!-- Second Linking -->

		<Node>
			<Parent>set_output</Parent>
			<Child>output_servo</Child>
			<Child>output_red</Child>
			<Child>output_green</Child>
			<Child>output_blue</Child>
			<Child>output_volume</Child>
			<Child>output_frequency</Child>
		</Node>

		<Node>
			<Parent>set_proportion</Parent>
			<Child>proportion_servo</Child>
			<Child>proportion_red</Child>
			<Child>proportion_green</Child>
			<Child>proportion_blue</Child>
			<Child>proportion_volume</Child>
			<Child>proportion_frequency</Child>
		</Node>

		<Node>
			<Parent>remove_relationship</Parent>
			<Child>remove_servo</Child>
			<Child>remove_red</Child>
			<Child>remove_green</Child>
			<Child>remove_blue</Child>
			<Child>remove_volume</Child>
			<Child>remove_frequency</Child>
		</Node>
		
	</Linking>
	
</Guide>


























